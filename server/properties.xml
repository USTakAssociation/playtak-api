<playtak.com>
  <email>
    <host>smtp.mailprovider.com</host>
    <user>userid@domain.com</user>
    <password>password</password>
    <from>userid@domain.com</from>
  </email>

  <irc>
    <enabled>false</enabled>
    <server>irc.freenode.net</server>
    <nick>nickname</nick>
    <login>nickname</login>
    <channel>#channel</channel>
    <password>password</password>
  </irc>

  <game>
    <reconnection-time>120</reconnection-time>
  </game>
  
  <server-settings>
    <port>10000</port>
    <portws>9999</portws>
    <porthttp>9998</porthttp>
    <db-host>host.docker.internal</db-host>
    <db-port>3306</db-port>
    <db-username>dbuser</db-username>
    <db-password>mydb123</db-password>
    <db-name>playtak_db</db-name>
    <db-path>/playtakdb/</db-path>
    <event-subscriber-url></event-subscriber-url>
  </server-settings>
</playtak.com>
